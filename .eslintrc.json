{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "google",
        "plugin:prettier/recommended"
    ],
    "plugins": ["prettier", "import"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "off",
        "no-unused-vars": "warn",
        "indent": [2, 4]
    }
}